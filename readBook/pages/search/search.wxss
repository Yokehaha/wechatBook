page{background:#f0f0f0;}
.change{height:100px;background:#f0f0f0;padding:10px 20px;}
.changeTop{width:100%;height:20px;}
.changeIcon{float:right;}
.changeBottom>view{margin-top:10px;}
.changeBottom{width:100%;height:80px;}
.changeText{float:left;margin:3px 5px;width:auto;height:23px;border:1px solid #ccc;padding:0 5px;text-align:center;border-radius:20px;}

.history{width:100%;background:#f0f0f0;height:auto;padding:10px 20px;}
.historyText{margin-bottom:10px;}
.historyRecord>text{font-size:16px;float:left;}
.historyClear{float:right;position:relative;left:-64rpx;top:-45rpx;}
.historyClear>text{font-size:14px;color:rgba(52, 152, 254, 1);}
.liClass { 
  width: 300px;
  font-size: 14px; 
  line-height: 34px; 
  color: #575757; 
  height: 34px; 
  display: block; 
  padding-left: 18px; 
  background-color: #fff; 
  margin-top: 3px;
 } 
